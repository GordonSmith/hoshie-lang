/*  
    Pipelines inside other pipelines.
    A pipeline declaration is just another "activity"
*/

data = readJson("../samples/people.json");

startCount = count();
endCount = count();

myProcess1 = pipeline(
    startCount, 
    skipN(3)
); 

myProcess2 = pipeline(
    firstN(3),
    endCount
);

myProcess = pipeline(myProcess1, myProcess2);

myProcess(data);

startCount;
endCount;
