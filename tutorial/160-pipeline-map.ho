/*  
    Pipeline Group and GroupCount.
*/

import { Person } from "./140-pipeline-filter";

PersonExt = {
    string fname, 
    string lname,
    number age,
    boolean isPensioner,
    boolean isChild
};

toPersonEx = (Person row) => {
    return {
        row.fname,
        row.lname,
        row.age,
        row.age < 18,
        row.age >= 65
    } typeof PersonExt;
};

mapPerson = pipeline(map(toPersonEx));

data = readJson("./data/people.json");

mapPerson(data);
